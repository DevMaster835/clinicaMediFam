	<queryString>
		<![CDATA[SELECT facturacion.idFacturacion, facturacion.fechaFactura, empleados.nombres AS nombreE, empleados.apellidos as apellidoE, pacientes.nombres as nombreP, pacientes.apellidos as apellidoP, detalle_facturacion.idProducto as idP, productos.nombre as Producto, productos.idPrecioHis as Precio, detalle_facturacion.cantidad, (productos.idPrecioHis*detalle_facturacion.cantidad) as SubtotalP, detalle_servicios.idServicio as idS, servicios.servicio as Servicio, servicios.precio as Precio, detalle_servicios.cantidad, (servicios.precio*detalle_servicios.cantidad) as SubtotalS FROM facturacion,empleados,pacientes, productos,detalle_facturacion, servicios,detalle_servicios WHERE facturacion.idEmpleado=empleados.idEmpleado and facturacion.idPaciente=pacientes.idPaciente and detalle_facturacion.idProducto=productos.idProducto AND detalle_facturacion.idFacturacion=facturacion.idFacturacion and detalle_servicios.idFacturacion=facturacion.idFacturacion and facturacion.idFacturacion='15' GROUP by detalle_facturacion.idProducto, detalle_servicios.idServicio]]>
